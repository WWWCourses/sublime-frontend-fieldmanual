{
  "ecmaVersion": 5,
  "libs": [
    "browser",
    "underscore"
  ],

  "loadEagerly": [
    "src/app/**/*.js"
  ], 

  "dontLoad": [
    "src/app/lib/**/**"
  ],
  
  "plugins": {
    "complete_strings": {},
    "node": {},
    "lint": {},
    "angular": {},
    "requirejs": {},
    "es_modules": {},
    "doc_comment": {
      "fullDocs": true
    }
  }
}